<div id="all-years" class="container choose-quarter">
    {{#each quarters}}
        <div class="col-xs-4 {{qState}}">
            <h5>{{Q}}<br/>{{year}}</h5>
            <ul>
                {{#each classes}}
                    <li>{{this}}</li>
                {{/each}}
                <a id="{{id}}" class="addToQuarter" href="#"><button class="btn btn-success ">Add Here</button></a>
            </ul>

        </div>
    {{/each}}
    <div class="choosenClass" style="display:none;">{{choosenClass}}</div>
</div>